rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Rules untuk koleksi kedai
    match /kedai/{document=**} {
      // User yang sudah login dapat membaca dan menulis data kedai mereka sendiri
      allow read, write: if request.auth != null;
    }

    // Rules untuk koleksi lainnya
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}

